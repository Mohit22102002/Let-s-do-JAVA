
import java.util.Scanner;


public class ComMinToYrs {
   public static void main(String[] args){
   int n,yrs;
   int days;
   yrs=0;
   days=0;
   Scanner s= new Scanner(System.in);
   n=s.nextInt();
   if(n>=525600)
   {
   yrs=n/525600;    
   }
   else
   {
   days=n%1440;
   }
   days=(n-(yrs*525600))/1440;
   System.out.println("Years="+yrs); 
   System.out.println("Days="+days); 
}
}
